<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(logo) -%>">
  <span class='title'>
    <%= logo.title %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.logos_logo_path(logo),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_logos_admin_logo_path(logo),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.logos_admin_logo_path(logo),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => logo.title),
        :method => :delete %>
  </span>
</li>
